<div class="col-lg-4 col-md-6 card">
   <div class="panel b">
      <div class="panel-heading">
         <div class="pull-right">
            <%#= render partial: "streams/stream_status", locals: { stream: stream} %>
            <!-- some status -->
         </div>
         <h4 class="m0"><%= step.name %></h4>
         <small class="text-muted">
            <%#= t(:Step) %> <%#= step.name %>
         </small>
      </div>
      

      <!-- START higher update -->
      <table class="table">
         <tbody>
            <tr>
               <td>
                  <% step.streams.each do |stream| %>
                  	<%#= stream.title %>
                  	<%= render partial: "steps/stream_status",
                  						 locals: { stream: stream } %>
                  <% end %>
               </td>
            </tr>
         </tbody>
      </table>
   </div>
   <a class="card-link" href="<%= link %>" ></a>
</div>